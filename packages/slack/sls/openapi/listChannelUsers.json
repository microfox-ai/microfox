                "type": "object",
                "description": "Body of the listChannelUsers sls call",
                "properties": {
                  "arguments": {
                    "type": "array",
                    "description": "Container for function arguments.",
                    "properties": {
                      "channelId": {
                        "type": "string",
                        "description": "channelId: The ID of the channel to get the members of."
                      }
                    },
                    "required": [
