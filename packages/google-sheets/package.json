{
  "name": "@microfox/google-sheets",
  "version": "1.2.1",
  "description": "A TypeScript SDK for interacting with Google Sheets.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"./**/*.ts*\"",
    "prettier-check": "prettier --check \"./**/*.ts*\""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@microfox/google-oauth": "^1.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@microfox/tsconfig": "*",
    "@types/jest": "^29.5.14",
    "@types/node": "^18",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "tsup": "^8",
    "typescript": "5.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "homepage": "https://github.com/microfox-ai/microfox",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microfox-ai/microfox.git"
  },
  "bugs": {
    "url": "https://github.com/microfox-ai/microfox/issues"
  },
  "keywords": [
    "google",
    "sheets",
    "typescript",
    "spreadsheet",
    "microfox",
    "sdk"
  ]
}
