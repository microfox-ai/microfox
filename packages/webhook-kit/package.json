{
  "name": "@microfox/webhook-kit",
  "version": "1.1.0",
  "description": "A central webhook kit for building webhook integrations in Microfox",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"./**/*.ts*\"",
    "prettier-check": "prettier --check \"./**/*.ts*\""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@microfox/webhook-core": "*",
    "@microfox/webhook-octokit": "*",
    "@microfox/webhook-slack": "*",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@microfox/tsconfig": "*",
    "@octokit/webhooks-types": "^7.3.1",
    "@types/node": "^18",
    "tsup": "^8",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/microfox-ai/microfox",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microfox-ai/microfox.git"
  },
  "bugs": {
    "url": "https://github.com/microfox-ai/microfox/issues"
  },
  "keywords": [
    "microfox",
    "webhook",
    "webhook-kit",
    "sdk",
    "typescript",
    "internal"
  ],
  "author": "TheMoonDevs",
  "license": "MIT"
}
